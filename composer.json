{
    "name":         "vimishor/cnp-php",
    "type":         "library",
    "description":  "Personal Identification Number (Cod Numeric Personal) validation",
    "homepage":     "https://github.com/vimishor/cnp-php",
    "keywords":     ["CNP", "romania",  "identification"],
    "license":      "MIT",
    "authors": [
        {
            "name":     "Alexandru Guzinschi",
            "email":    "alex@gentle.ro",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "gentle/embeddables": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Vimishor\\Cnp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vimishor\\Cnp\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}